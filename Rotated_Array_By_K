using System;
using System.Linq;

public class HelloWorld
{
    public static void Main(string[] args)
    {
      int[] arr = {1, 2, 3, 4, 5};
      int k = 2;   
       int n = arr.Length;
      int [] final = new int[n];
   
      
     
      for(int i = 0;i<n;i++){
           int index = (i+k)%n;
           final[index] = arr[i];
          
      }
      for(int i = 0; i<n ; i++){
          Console.WriteLine("After Move Forward : " + final[i]);
      }
       
       
    }
}
