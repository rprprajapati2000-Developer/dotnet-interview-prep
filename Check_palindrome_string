using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
       string input= "madam";
       string rev = "";
       for(int i = input.Length-1; i>=0;i--){
           
           rev += input[i]; 
       }
       if(rev == input){
       
       Console.WriteLine("The Inpute Is Palindrome ");
       }
       else {
           Console.WriteLine("The Inpute Is not Palindrome ");
       }
      
               
               
          
       
    }
}
