using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
       string input= "hello";
       string rev = "";
       for(int i = input.Length-1; i>=0;i--){
           
           rev += input[i]; 
       }
       
       Console.WriteLine("Reverse Result Is : " + String.Join(",",rev.ToCharArray()));
      
               
               
          
       
    }
}
