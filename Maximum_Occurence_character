using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
       string s1=  "aabbbccccc";
       int [] freq = new int[26];
       int max = 0;
       int index = 0;
       
       foreach(char ch in s1){
           freq[ch-'a']++;
       }
       for(int i = 0 ; i<26;i++){
           if(max<freq[i])
           {
               max = freq[i];
               index = i;
               
           }
           
           
       }
       Console.WriteLine("The Maximum Occurence is : " + (char)(index+'a'));
       
    }
}
